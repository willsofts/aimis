	<div class="col-md-12 col-height">
			<div class="table-layer-class">
				<div class="row row-height">
					<div class="col-height col-md-12">
<label for="correctprompt" id="correctprompt_label" class="control-label"><%=label.get('correctprompt_label','Correct Prompt Info')%></label>
<div class="input-group"><textarea id="correctprompt" name="correctprompt" class="form-control input-md" rows="2"></textarea><div class="input-group-required"><label class="required">&#160;</label></div></div><div id="correctprompt_alert" role="alert" class="has-feedback has-error fa-hidden"><%=label.get('correctprompt_alert','You can not leave this empty')%></div>
					</div>
				</div>
			</div>
	</div>
</div>
