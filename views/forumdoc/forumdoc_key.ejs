<% let display = page.isUpdateMode()?"readonly":""; %>
		<input type="hidden" id="forumcode" name="forumcode" value="<%=data.dataset?.forumcode%>" />
		<input type="hidden" id="forumgroup" name="forumgroup" value="DOC" />
		<div class="row row-height">
			<div class="col-md-12 col-height">
					<div class="table-layer-class">
						<div class="row row-height">
							<div class="col-height col-md-7">
<label for="forumid" id="forumid_label" class="control-label"><%=label.get('forumid_label','ID')%></label>
<div class="input-group"><input type="text" id="forumid" name="forumid" class="form-control input-md irequired alert-input" class="form-control input-md irequired alert-input" maxlength="50" value="<%=data.dataset?.forumid%>" <%=display%>/><span></span><div class="input-group-required"><label class="required">*</label></div></div><div id="forumid_alert" role="alert" class="has-feedback has-error fa-hidden"><%=label.get('forumid_alert','You can not leave this empty')%></div>
							</div>
							<div class="col-height col-md-3">
								<div class="checkbox checkbox form-check" id="shareflagbox">									
									<input type="checkbox" id="shareflag" name="shareflag" class="form-control input-md form-check-input" class="form-control input-md" value="1" <%=page.checked('1',data.dataset?.shareflag)%> />
									<label for="shareflag" id="shareflag_label" class="control-label form-check-label"><%=label.get('shareflag_label','Sharing')%></label>
								</div>
							</div>
						</div>
					</div>
			</div>
		</div>
		<div class="row row-height">
			<div class="col-md-12 col-height">
					<div class="table-layer-class">
						<div class="row row-height">
							<div class="col-height col-md-10">
<label for="forumtitle" id="forumtitle_label" class="control-label"><%=label.get('forumtitle_label','Title')%></label>
<div class="input-group"><input type="text" id="forumtitle" name="forumtitle" class="form-control input-md irequired alert-input" maxlength="50" value="<%=data.dataset?.forumtitle%>" /><span></span><div class="input-group-required"><label class="required">*</label></div></div><div id="forumtitle_alert" role="alert" class="has-feedback has-error fa-hidden"><%=label.get('forumtitle_alert','You can not leave this empty')%></div>
							</div>
						</div>
					</div>
			</div>
		</div>
